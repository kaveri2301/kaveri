package day0;

 class parent {
	void add(int x,int y)
	{
		System.out.println("add from parent1 is "+(x+y));
	}

}

public class child extends parent
  {
	 void add(int x, int y)
	 
	 {
 super.add(5,5);
 }
	 
 public static void main (String[] args) {
	 child obj = new child();
	 obj.add (5,5);
 }
 }
